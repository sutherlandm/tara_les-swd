FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../../../Setup/caseSetup"

convertToMeters 1;

//Points defininig initial size
xi $x_start;
xf $x_end;
yi $y_start;
yf $y_end;
zi $z_start;
zf $z_end;


vertices
(
    ($xi $yi $zi) //0
    ($xf $yi $zi) //1
    ($xf $yf $zi) //2
    ($xi $yf $zi) //3
    ($xi $yi $zf) //4
    ($xf $yi $zf) //5
    ($xf $yf $zf) //6
    ($xi $yf $zf) //7
);


blocks
(
	hex (0 1 2 3 4 5 6 7)  //vertex numbers
	($x $y $z)    		   //numbe of cell in x,y,z
	simpleGrading (1 1 1)  //cell expansion ratios
);

edges
(
);

boundary
(
	//Include the LES inlet condition
	#include "../../../Setup/inlet_RANS"

	outlet
	{
		type patch; 
		faces
		(
		(1 5 6 2)
		);
	}

	ground
	{
		type wall;
		faces
		(		
		(3 0 1 2)
		);
	}

	top
	{
		type patch; 
		faces
		(
		(3 7 6 2)
		);
	}

	sides
	{
		type patch; 
		faces
		(
		(7 4 5 6)
		(0 4 5 1)
		);
	}
);

